  <PartID>1087836159</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub Opening()</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Validate required inputs</SourceLine>
   <SourceLine>If App.SupportEngineKitURL = "https://yourdomain.com/api/" Or App.SupportEngineKitURL.Trim = "" Then</SourceLine>
   <SourceLine>// Handle missing URL - could log error or use default behavior</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Safe URL construction</SourceLine>
   <SourceLine>Var url As String</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>url = App.SupportEngineKitURL.Trim</SourceLine>
   <SourceLine>If Not url.EndsWith("/") Then</SourceLine>
   <SourceLine>url = url + "/"</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>url = url + "ticket_categories.php"</SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
   <SourceLine>// Handle URL construction error</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Create connection safely</SourceLine>
   <SourceLine>Var conn As URLConnection</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>conn = New URLConnection</SourceLine>
   <SourceLine>If conn = Nil Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>conn.RequestHeader("Accept") = "application/json"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Send request with error handling</SourceLine>
   <SourceLine>Var response As String</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>response = conn.SendSync("GET", url)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Validate response</SourceLine>
   <SourceLine>If response = "" Or response.Trim = "" Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Parse JSON response safely</SourceLine>
   <SourceLine>Var json As JSONItem</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>json = New JSONItem(response)</SourceLine>
   <SourceLine>If json = Nil Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Check for success response</SourceLine>
   <SourceLine>If Not json.HasKey("success") Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var successValue As Variant = json.Value("success")</SourceLine>
   <SourceLine>If successValue = Nil Or successValue &lt;&gt; True Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Validate categories data exists</SourceLine>
   <SourceLine>If Not json.HasKey("categories") Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var categoriesValue As Variant = json.Value("categories")</SourceLine>
   <SourceLine>If categoriesValue = Nil Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Safe conversion to JSONItem</SourceLine>
   <SourceLine>Var categoriesJSON As JSONItem</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>categoriesJSON = categoriesValue</SourceLine>
   <SourceLine>If categoriesJSON = Nil Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Validate PopupMenuCategories exists</SourceLine>
   <SourceLine>If PopupMenuCategories = Nil Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Clear existing rows safely</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>PopupMenuCategories.RemoveAllRows</SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Process categories with comprehensive validation</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>Var categoryCount As Integer = categoriesJSON.Count</SourceLine>
   <SourceLine>If categoryCount &lt;= 0 Then</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>For i As Integer = 0 To categoryCount - 1</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>Var cat As JSONItem = categoriesJSON.Child(i)</SourceLine>
   <SourceLine>If cat = Nil Then</SourceLine>
   <SourceLine>Continue // Skip this iteration if child is nil</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Safely extract category name</SourceLine>
   <SourceLine>If Not cat.HasKey("name") Then</SourceLine>
   <SourceLine>Continue</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var nameValue As Variant = cat.Value("name")</SourceLine>
   <SourceLine>If nameValue = Nil Then</SourceLine>
   <SourceLine>Continue</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var catName As String</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>catName = nameValue.StringValue</SourceLine>
   <SourceLine>If catName = "" Or catName.Trim = "" Then</SourceLine>
   <SourceLine>Continue</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>catName = catName.Trim</SourceLine>
   <SourceLine>Catch</SourceLine>
   <SourceLine>Continue</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Safely extract category ID</SourceLine>
   <SourceLine>If Not cat.HasKey("id") Then</SourceLine>
   <SourceLine>Continue</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var idValue As Variant = cat.Value("id")</SourceLine>
   <SourceLine>If idValue = Nil Then</SourceLine>
   <SourceLine>Continue</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var catID As Integer</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>catID = idValue.IntegerValue</SourceLine>
   <SourceLine>Catch</SourceLine>
   <SourceLine>Continue</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Add row safely</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>PopupMenuCategories.AddRow(catName)</SourceLine>
   <SourceLine>Var lastIndex As Integer = PopupMenuCategories.LastAddedRowIndex</SourceLine>
   <SourceLine>If lastIndex &gt;= 0 Then</SourceLine>
   <SourceLine>PopupMenuCategories.RowTagAt(lastIndex) = catID</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
   <SourceLine>// Continue processing other categories even if one fails</SourceLine>
   <SourceLine>Continue</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
   <SourceLine>// Continue with next category if current one fails</SourceLine>
   <SourceLine>Continue</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Only show controls if we successfully added categories</SourceLine>
   <SourceLine>If PopupMenuCategories.RowCount &gt; 0 Then</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>If LabelCategory &lt;&gt; Nil Then</SourceLine>
   <SourceLine>LabelCategory.Visible = True</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>PopupMenuCategories.Visible = True</SourceLine>
   <SourceLine>PopupMenuCategories.SelectedRowIndex = 0</SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
   <SourceLine>// Handle UI update errors gracefully</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Catch e As RuntimeException</SourceLine>
