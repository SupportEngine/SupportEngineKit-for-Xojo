 <ControlBehavior>
  <Superclass>DesktopButton</Superclass>
  <HookInstance>
   <ItemName>Pressed</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>713191423</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub Pressed()</SourceLine>
    <SourceLine>Var selectedIndex As Integer = PopupMenuCategories.SelectedRowIndex</SourceLine>
    <SourceLine>Var Category As String </SourceLine>
    <SourceLine>me.Enabled=false</SourceLine>
    <SourceLine>ProgressWheelSending.Visible=true</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if PopupMenuCategories.Visible=true then</SourceLine>
    <SourceLine>If selectedIndex &gt;= 0 Then</SourceLine>
    <SourceLine>Category = PopupMenuCategories.RowTagAt(selectedIndex).StringValue</SourceLine>
    <SourceLine>// Use selectedCategoryID when creating a ticket</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>Else</SourceLine>
    <SourceLine>Category = ""</SourceLine>
    <SourceLine>End if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Var Priority as String</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if PopupMenuPriority.SelectedRowIndex = 0 then</SourceLine>
    <SourceLine>Priority = "low"</SourceLine>
    <SourceLine>End if</SourceLine>
    <SourceLine>if PopupMenuPriority.SelectedRowIndex = 1 then</SourceLine>
    <SourceLine>Priority = "medium"</SourceLine>
    <SourceLine>End if</SourceLine>
    <SourceLine>if PopupMenuPriority.SelectedRowIndex = 2 then</SourceLine>
    <SourceLine>Priority = "high"</SourceLine>
    <SourceLine>End if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Var CheckResult as Boolean</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>CheckResult = App.SupportEngineKitCreate(TextFieldCompany.text, TextFieldName.text, TextFieldEmail.text, Priority, TextFieldSubject.text, TextAreaMessage.text, Category)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if CheckResult = true then</SourceLine>
    <SourceLine>ProgressWheelSending.Visible=false</SourceLine>
    <SourceLine>MessageBox("Ticket received. Thanks for your feedback!")</SourceLine>
    <SourceLine>SupportEngineNewTicket.Close</SourceLine>
    <SourceLine>else</SourceLine>
    <SourceLine>me.Enabled=true</SourceLine>
    <SourceLine>ProgressWheelSending.Visible=false</SourceLine>
    <SourceLine>MessageBox("Error: " + App.SupportEngineKitError)</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
