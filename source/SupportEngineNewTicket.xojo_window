#tag Events ButtonSend
	#tag Event
		Sub Pressed()
		  Var selectedIndex As Integer = PopupMenuCategories.SelectedRowIndex
		  Var Category As String 
		  me.Enabled=false
		  ProgressWheelSending.Visible=true
		  
		  if PopupMenuCategories.Visible=true then
		    If selectedIndex >= 0 Then
		      Category = PopupMenuCategories.RowTagAt(selectedIndex).StringValue
		      // Use selectedCategoryID when creating a ticket
		    End If
		  Else
		    Category = ""
		  End if
		  
		  Var Priority as String
		  
		  if PopupMenuPriority.SelectedRowIndex = 0 then
		    Priority = "low"
		  End if
		  if PopupMenuPriority.SelectedRowIndex = 1 then
		    Priority = "medium"
		  End if
		  if PopupMenuPriority.SelectedRowIndex = 2 then
		    Priority = "high"
		  End if
		  
		  Var CheckResult as Boolean
		  
		  CheckResult = App.SupportEngineKitCreate(TextFieldCompany.text, TextFieldName.text, TextFieldEmail.text, Priority, TextFieldSubject.text, TextAreaMessage.text, Category)
		  
		  if CheckResult = true then
		    ProgressWheelSending.Visible=false
		    MessageBox("Ticket received. Thanks for your feedback!")
		    SupportEngineNewTicket.Close
		  else
		    me.Enabled=true
		    ProgressWheelSending.Visible=false
		    MessageBox("Error: " + App.SupportEngineKitError)
		  end if
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
