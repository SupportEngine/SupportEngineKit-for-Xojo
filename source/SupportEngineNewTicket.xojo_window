#tag WindowCode
	#tag Event
		Sub Opening()
		  Try
		    
		    // Validate required inputs
		    If App.SupportEngineKitURL = "https://yourdomain.com/api/" Or App.SupportEngineKitURL.Trim = "" Then
		      // Handle missing URL - could log error or use default behavior
		      Return
		    End If
		    
		    // Safe URL construction
		    Var url As String
		    Try
		      url = App.SupportEngineKitURL.Trim
		      If Not url.EndsWith("/") Then
		        url = url + "/"
		      End If
		      url = url + "ticket_categories.php"
		    Catch e As RuntimeException
		      // Handle URL construction error
		      Return
		    End Try
		    
		    // Create connection safely
		    Var conn As URLConnection
		    Try
		      conn = New URLConnection
		      If conn = Nil Then
		        Return
		      End If
		      
		      conn.RequestHeader("Accept") = "application/json"
		      
		    Catch e As RuntimeException
		      Return
		    End Try
		    
		    // Send request with error handling
		    Var response As String
		    Try
		      response = conn.SendSync("GET", url)
		      
		      // Validate response
		      If response = "" Or response.Trim = "" Then
		        Return
		      End If
		    Catch e As RuntimeException
		      Return
		    End Try
		    
		    // Parse JSON response safely
		    Var json As JSONItem
		    Try
		      json = New JSONItem(response)
		      If json = Nil Then
		        Return
		      End If
		    Catch e As RuntimeException
		      Return
		    End Try
		    
		    // Check for success response
		    If Not json.HasKey("success") Then
		      Return
		    End If
		    
		    Var successValue As Variant = json.Value("success")
		    If successValue = Nil Or successValue <> True Then
		      Return
		    End If
		    
		    // Validate categories data exists
		    If Not json.HasKey("categories") Then
		      Return
		    End If
		    
		    Var categoriesValue As Variant = json.Value("categories")
		    If categoriesValue = Nil Then
		      Return
		    End If
		    
		    // Safe conversion to JSONItem
		    Var categoriesJSON As JSONItem
		    Try
		      categoriesJSON = categoriesValue
		      If categoriesJSON = Nil Then
		        Return
		      End If
		    Catch e As RuntimeException
		      Return
		    End Try
		    
		    // Validate PopupMenuCategories exists
		    If PopupMenuCategories = Nil Then
		      Return
		    End If
		    
		    // Clear existing rows safely
		    Try
		      PopupMenuCategories.RemoveAllRows
		    Catch e As RuntimeException
		      Return
		    End Try
		    
		    // Process categories with comprehensive validation
		    Try
		      Var categoryCount As Integer = categoriesJSON.Count
		      If categoryCount <= 0 Then
		        Return
		      End If
		      
		      For i As Integer = 0 To categoryCount - 1
		        Try
		          Var cat As JSONItem = categoriesJSON.Child(i)
		          If cat = Nil Then
		            Continue // Skip this iteration if child is nil
		          End If
		          
		          // Safely extract category name
		          If Not cat.HasKey("name") Then
		            Continue
		          End If
		          
		          Var nameValue As Variant = cat.Value("name")
		          If nameValue = Nil Then
		            Continue
		          End If
		          
		          Var catName As String
		          Try
		            catName = nameValue.StringValue
		            If catName = "" Or catName.Trim = "" Then
		              Continue
		            End If
		            catName = catName.Trim
		          Catch
		            Continue
		          End Try
		          
		          // Safely extract category ID
		          If Not cat.HasKey("id") Then
		            Continue
		          End If
		          
		          Var idValue As Variant = cat.Value("id")
		          If idValue = Nil Then
		            Continue
		          End If
		          
		          Var catID As Integer
		          Try
		            catID = idValue.IntegerValue
		          Catch
		            Continue
		          End Try
		          
		          // Add row safely
		          Try
		            PopupMenuCategories.AddRow(catName)
		            Var lastIndex As Integer = PopupMenuCategories.LastAddedRowIndex
		            If lastIndex >= 0 Then
		              PopupMenuCategories.RowTagAt(lastIndex) = catID
		            End If
		          Catch e As RuntimeException
		            // Continue processing other categories even if one fails
		            Continue
		          End Try
		          
		        Catch e As RuntimeException
		          // Continue with next category if current one fails
		          Continue
		        End Try
		      Next
		      
		      // Only show controls if we successfully added categories
		      If PopupMenuCategories.RowCount > 0 Then
		        Try
		          If LabelCategory <> Nil Then
		            LabelCategory.Visible = True
		          End If
		          PopupMenuCategories.Visible = True
		          PopupMenuCategories.SelectedRowIndex = 0
		        Catch e As RuntimeException
		          // Handle UI update errors gracefully
		        End Try
		      End If
		      
		    Catch e As RuntimeException
		      // Handle any errors in category processing
		      Return
		    End Try
		    
		  Catch e As RuntimeException
		    // Handle any unexpected errors at the top level
		    Return
		  End Try
		End Sub
	#tag EndEvent


